<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="tech.ouyezi.logsdk">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application android:allowBackup="true" android:label="@string/app_name"
        android:supportsRtl="true">
        <meta-data
            android:name="LOG_DIR_NAME"
            android:value="${LOG_DIR_NAME}"/>
        <meta-data
            android:name="LOG_FILE_NAME"
            android:value="${LOG_FILE_NAME}"/>
        <meta-data
            android:name="LOG_ROOT_LEVEL"
            android:value="${LOG_ROOT_LEVEL}"/>
    </application>

</manifest>
